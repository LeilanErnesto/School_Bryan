@model school.web.Models.StudentGradeListModel

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Codigo de Inscripcion
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EnrollmentId)
        </dd>
        <dt class = "col-sm-2">
            Codigo de Curso
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CourseId)
        </dd>
        <dt class = "col-sm-2">
            Codigo de Estudiante
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.StudentId)
        </dd>
        <dt class = "col-sm-2">
            Grado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Grade)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.StudentId }) |
    <a asp-action="Index">Volver a la lista</a>
</div>
